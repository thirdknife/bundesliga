{
  "name": "quixelsuite",
  "version": "0.0.1",
  "description": "Quixel Suite 3",
  "repository": {
    "type": "git",
    "url": "https://nofelmahmood@bitbucket.org/quixeltools/quixel-suite.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "better-npm-run build",
    "dev": "better-npm-run dev",
    "test": "echo \"Error: no test specified\" && exit 1",
    "update": "better-npm-run update",
    "watch-client": "better-npm-run watch-client"
  },
  "betterScripts": {
    "build": {
      "command": "node ./node_modules/webpack/bin/webpack.js --verbose --colors --display-error-details --config webpack/prod.config.js",
      "env": {
        "UV_THREADPOOL_SIZE": "100"
      }
    },
    "dev": "node ./node_modules/concurrently/src/main.js --kill-others \"npm run watch-client\"",
    "update": "git checkout develop && git pull origin develop && npm install && npm run build",
    "watch-client": {
      "command": "node webpack/webpack-dev-server.js",
      "env": {
        "NODE_PATH": "./src",
        "UV_THREADPOOL_SIZE": "100"
      }
    }
  },
  "keywords": [
    "3",
    "quixel",
    "suite"
  ],
  "author": "Nofel Mahmood",
  "license": "ISC",
